<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <managed-bean>
  <managed-bean-name>kitapBean</managed-bean-name>
  <managed-bean-class>KitapBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>demoBean</managed-bean-name>
  <managed-bean-class>DemoBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>yoneticiGiris</managed-bean-name>
  <managed-bean-class>YoneticiGiris</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>demoView</managed-bean-name>
  <managed-bean-class>DemoView</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>grafik</managed-bean-name>
  <managed-bean-class>Grafik</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/Giris.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>dogru</from-outcome>
   <to-view-id>/Anasayfa2.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>UyeOl</from-outcome>
   <to-view-id>/UyeOl.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/UyeOl.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>geri</from-outcome>
   <to-view-id>/Giris.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/Anasayfa2.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>Anasayfa2</from-outcome>
   <to-view-id>/Anasayfa2.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>YoneticiGiris</from-outcome>
   <to-view-id>/YoneticiGiris.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>Arsiv</from-outcome>
   <to-view-id>/Arsiv.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>Kitap</from-outcome>
   <to-view-id>/Kitap.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/YoneticiGiris.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>gir</from-outcome>
   <to-view-id>/KitapYonetici.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
</faces-config>
